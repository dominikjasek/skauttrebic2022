{
  "name": "skautrebic",
  "private": true,
  "version": "0.1.0",
  "description": "CMS pro skauttrebic.cz",
  "scripts": {
    "translate-admin": "cp -a ./src/admin/translations/admin/. ./node_modules/@strapi/admin/admin/src/translations/",
    "translate-upload-plugin": "cp -a ./src/admin/translations/plugins/upload/. ./node_modules/@strapi/plugin-upload/admin/src/translations",
    "translate-strapi-plugin-content-type-builder": "cp -a ./src/admin/translations/plugins/plugin-content-type-builder/. ./node_modules/@strapi/plugin-content-type-builder/admin/src/translations",
    "translate": "npm run translate-admin && npm run translate-strapi-plugin-content-type-builder && npm run translate-strapi-plugin-content-type-builder",
    "cs": "config-sync",
    "cs-export": "yarn cs e -y",
    "dev": "strapi develop",
    "start": "strapi start",
    "build": "yarn translate && yarn strapi ts:generate-types && strapi build",
    "strapi": "strapi",
    "lint": "eslint"
  },
  "imports": {
    "#config/*": "./config/*",
    "#src/*": "./src/*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "8.25.0"
  },
  "dependencies": {
    "@_sh/strapi-plugin-ckeditor": "^1.1.2",
    "@strapi/plugin-color-picker": "^4.4.0",
    "@strapi/plugin-documentation": "^4.4.0",
    "@strapi/plugin-graphql": "^4.3.9",
    "@strapi/plugin-i18n": "^4.3.9",
    "@strapi/plugin-users-permissions": "^4.3.9",
    "@strapi/provider-upload-aws-s3": "^4.3.9",
    "@strapi/strapi": "^4.3.9",
    "better-sqlite3": "7.6.2",
    "strapi-plugin-comments": "^2.1.8",
    "strapi-plugin-config-sync": "^1.0.4",
    "strapi-plugin-email-designer": "^2.1.1",
    "strapi-provider-email-sendinblue": "^4.0.0"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "e2a087ca-04cc-46f3-a6bf-16984201564a"
  },
  "engines": {
    "node": "14.20.0",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
